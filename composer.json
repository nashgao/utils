{
    "name": "nashgao/utils",
    "authors": [
        {
            "name": "nashgao",
            "email": "nashgao1992@gmail.com"
        }
    ],
    "license": "MIT",
    "description": "",
    "require": {
        "php": ">=8.0",
        "hyperf/di": "2.2.*",
        "hyperf/snowflake": "2.2.*",
        "hyperf/contract": "2.2.*",
        "hyperf/event": "2.2.*",
        "easyswoole/spl": "1.3.*"
    },
    "require-beta": {
        "swoole/ide-helper": "~4.6",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.12.90"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nashgao\\Utils\\": "src"
        },
        "files": [
            "src/Helper/hyperf.php",
            "src/Helper/function.php"
        ]
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src"
    }
}
